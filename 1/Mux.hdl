/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=notsel);
    Nand(a=notsel,b=a,out=n1);
    Nand(a=sel,b=b,out=n2);
    Nand(a=n1,b=n2,out=out);

}