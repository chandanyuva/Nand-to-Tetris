/**
 * Demultiplexor:
 * [a, b] = [in, 0] if sel = 0
 *          [0, in] if sel = 1
 */
CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not(in= sel, out= notsel);
    Nand(a= in, b= notsel, out= t1);
    Not(in= t1, out= a);

    Nand(a= in, b= sel, out= t2);
    Not(in= t2, out= b);
}